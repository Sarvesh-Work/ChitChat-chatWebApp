{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [

      { "src": "/api/signUp", "dest": "/api/index.js" },
      { "src": "/api/login", "dest": "/api/index.js" },
      
 
      { "src": "/api/verified/profile", "dest": "/api/index.js" },
      { "src": "/api/verified/logOut", "dest": "/api/index.js" },
      { "src": "/api/verified/notFriendSearch", "dest": "/api/index.js" },
      { "src": "/api/verified/sendFriendRequest", "dest": "/api/index.js" },
      { "src": "/api/verified/acceptFriendRequest", "dest": "/api/index.js" },
      { "src": "/api/verified/notifications", "dest": "/api/index.js" },
      { "src": "/api/verified/friends", "dest": "/api/index.js" },
      { "src": "/api/verified/editProfile", "dest": "/api/index.js" },
      
   
      { "src": "/api/verified/newGroup", "dest": "/api/index.js" },
      { "src": "/api/verified/addNewMembers", "dest": "/api/index.js" },
      { "src": "/api/verified/removeMember", "dest": "/api/index.js" },
      { "src": "/api/verified/leaveGroup/.*", "dest": "/api/index.js" },
      { "src": "/api/verified/.*?/changeAvatar", "dest": "/api/index.js" },
      { "src": "/api/verified/allGroups", "dest": "/api/index.js" },
      { "src": "/api/verified/allChats", "dest": "/api/index.js" },
      { "src": "/api/verified/messages/.*", "dest": "/api/index.js" },
      { "src": "/api/verified/attachments", "dest": "/api/index.js" },
      { "src": "/api/verified/.*", "dest": "/api/index.js" }
    ]
  }
  